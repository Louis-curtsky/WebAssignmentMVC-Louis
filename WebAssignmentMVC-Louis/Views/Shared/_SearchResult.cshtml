@model List<Person>

<div class="row">
    <div class="col">ID Number</div>
    <div class="col">First Name</div>
    <div class="col">Last Name</div>
    <div class="col">Telephone</div>
    <div class="col">City</div>
    <div class="col">Action</div>
</div>

    @for (int i = 0; i < Model.Count; i++)
    {
        <div class="row">
            <div class="col">
                <p>@Model[i].Id</p>
            </div>
            <div class="col">
                <p>@Model[i].FirstName</p>
            </div>
            <div class="col"poster>
                <p>@Model[i].LastName</p>
            </div>
            <div class="col">
                <p>@Model[i].City</p>
            </div>
            <div class="col">
                <p>@Model[i].Phone</p>
            </div>
            <div class="col">
                <p>
                    <a class="btn btn-info" asp-action="Delete" asp-controller="Person" asp-route-id="@Model[i].Id">Delete</a>
                    @*<a class="btn btn-info" asp-action="Detail" asp-controller="Person" asp-route-id="@Model[i].Id">Details</a>*@
                    <input name="ID" type="hidden" id="inputId" value="@Model[i].Id" />
                    <button onclick="findThroughId('@Url.Action("Detail", "Person", new {id=Model[i].Id})', 'inputId')">Detail</button>
                </p>
            </div>
        </div>
    }
