@model List<Person>
@{
    ViewData["Title"] = "Persons - Index";
}
<hr />
<h1>Person Information</h1>

<table class="contact">
    <tr>
        <th>ID Number</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Telephone</th>
        <th>City</th>
    </tr>
    <tbody>
        @foreach (Person person in Model)
        {
            <tr>
                <partial name="_PersonList" model="person" />
            </tr>
        }
    </tbody>
</table>
@if (ViewBag.msg != null)
{
    <h3>@ViewBag.msg</h3>
}

